</script>
</head>
<body>
    <div class="container">
        <div class="header">
            <h1>Claude Code Skills Installer</h1>
            <p>Select the skills you want to install - Ninite style!</p>
        </div>

        <div class="controls">
            <div class="search-box">
                <input type="text" id="searchInput" placeholder="Search skills...">
            </div>
            <div class="filter-buttons">
                <button class="filter-btn active" data-filter="all">All</button>
                <button class="filter-btn" data-filter="cloudflare">Cloudflare</button>
                <button class="filter-btn" data-filter="ai">AI/ML</button>
                <button class="filter-btn" data-filter="web">Web Dev</button>
                <button class="filter-btn" data-filter="devops">DevOps</button>
            </div>
        </div>

        <div class="stats">
            <div class="stat-item">
                <div class="stat-value" id="totalSkills">0</div>
                <div>Total Skills</div>
            </div>
            <div class="stat-item">
                <div class="stat-value" id="selectedSkills">0</div>
                <div>Selected</div>
            </div>
            <div class="stat-item">
                <div class="stat-value" id="filteredSkills">0</div>
                <div>Showing</div>
            </div>
        </div>

        <div class="skills-grid" id="skillsGrid">
            <div class="loading">Loading skills...</div>
        </div>

        <div class="controls">
            <div class="action-buttons">
                <button class="btn btn-secondary" onclick="selectAll()">Select All</button>
                <button class="btn btn-secondary" onclick="deselectAll()">Deselect All</button>
            </div>
            <div class="action-buttons">
                <button class="btn btn-primary" onclick="generateInstallScript()">Generate Install Script</button>
            </div>
        </div>

        <div class="footer">
            <p>Powered by Claude Code Tools Team | Select skills and click "Generate Install Script" to download</p>
        </div>
    </div>

    <script>
        let allSkills = [];
        let selectedSkills = new Set();
        let currentFilter = 'all';

        function initializeSkills() {
            // Skills data injected by build script via window.EMBEDDED_SKILLS
            if (window.EMBEDDED_SKILLS) {
                allSkills = window.EMBEDDED_SKILLS;
                console.log(`Loaded ${allSkills.length} skills from embedded data`);
            } else {
                console.error('Skills data not embedded! Run: .\\install-from-selector.ps1 -OpenBrowser');
                allSkills = [];
            }
            updateStats();
            renderSkills();
        }

        function renderSkills() {
            const grid = document.getElementById('skillsGrid');
            const searchTerm = document.getElementById('searchInput').value.toLowerCase();

            let filteredSkills = allSkills.filter(skill => {
                const matchesSearch = skill.name.toLowerCase().includes(searchTerm) ||
                                    skill.description.toLowerCase().includes(searchTerm);

                const matchesFilter = currentFilter === 'all' ||
                                    (skill.category && skill.category.toLowerCase().includes(currentFilter.toLowerCase()));

                return matchesSearch && matchesFilter;
            });

            document.getElementById('filteredSkills').textContent = filteredSkills.length;

            if (filteredSkills.length === 0) {
                grid.innerHTML = '<div class="loading">No skills found matching your criteria.</div>';
                return;
            }

            grid.innerHTML = filteredSkills.map(skill => `
                <div class="skill-card ${selectedSkills.has(skill.name) ? 'selected' : ''}"
                     onclick="toggleSkill('${skill.name}')">
                    <div class="checkbox"></div>
                    <div class="skill-name">${skill.name}</div>
                    <div class="skill-description">${skill.description}</div>
                    <div class="skill-tags">
                        ${skill.category ? `<span class="tag">${skill.category}</span>` : ''}
                    </div>
                </div>
            `).join('');
        }

        function toggleSkill(skillName) {
            if (selectedSkills.has(skillName)) {
                selectedSkills.delete(skillName);
            } else {
                selectedSkills.add(skillName);
            }
            updateStats();
            renderSkills();
        }

        function selectAll() {
            const searchTerm = document.getElementById('searchInput').value.toLowerCase();
            allSkills.forEach(skill => {
                const matchesSearch = skill.name.toLowerCase().includes(searchTerm) ||
                                    skill.description.toLowerCase().includes(searchTerm);
                const matchesFilter = currentFilter === 'all' ||
                                    (skill.category && skill.category.toLowerCase().includes(currentFilter.toLowerCase()));

                if (matchesSearch && matchesFilter) {
                    selectedSkills.add(skill.name);
                }
            });
            updateStats();
            renderSkills();
        }

        function deselectAll() {
            selectedSkills.clear();
            updateStats();
            renderSkills();
        }

        function updateStats() {
            document.getElementById('totalSkills').textContent = allSkills.length;
            document.getElementById('selectedSkills').textContent = selectedSkills.size;
        }

        function generateInstallScript() {
            console.log('[DEBUG] generateInstallScript called');
            console.log('[DEBUG] selectedSkills.size:', selectedSkills.size);
            console.log('[DEBUG] selectedSkills:', Array.from(selectedSkills));

            if (selectedSkills.size === 0) {
                alert('Please select at least one skill to install.');
                return;
            }

            const skillsList = Array.from(selectedSkills).join(',');
            const scriptContent = `# Claude Code Skills Installation Script
# Generated: ${new Date().toISOString()}
# Selected Skills: ${selectedSkills.size}

$selectedSkills = @(
${Array.from(selectedSkills).map(s => `    '${s}'`).join(',\n')}
)

Write-Host "Installing ${selectedSkills.size} selected skills..." -ForegroundColor Cyan

foreach ($skill in $selectedSkills) {
    Write-Host "Installing: $skill" -ForegroundColor Yellow
    # Adjust path as needed for your setup
    & "$PSScriptRoot\\..\\tools\\install-skills.ps1" -SkillName $skill
}

Write-Host "Installation complete!" -ForegroundColor Green
`;

            console.log('[DEBUG] Script content generated, length:', scriptContent.length);

            try {
                const blob = new Blob([scriptContent], { type: 'text/plain' });
                console.log('[DEBUG] Blob created:', blob.size, 'bytes');

                const url = URL.createObjectURL(blob);
                console.log('[DEBUG] Blob URL created:', url);

                const a = document.createElement('a');
                a.href = url;
                a.download = 'install-selected-skills.ps1';
                a.style.display = 'none';

                document.body.appendChild(a);
                console.log('[DEBUG] Link element added to DOM');

                // Force download with setTimeout to ensure it's treated as user-initiated
                setTimeout(() => {
                    console.log('[DEBUG] Triggering download...');
                    a.click();
                    console.log('[DEBUG] Click triggered');

                    setTimeout(() => {
                        document.body.removeChild(a);
                        URL.revokeObjectURL(url);
                        console.log('[DEBUG] Cleanup complete');
                    }, 100);
                }, 0);

                // Show success message
                alert(`Install script generated for ${selectedSkills.size} skill(s)!\n\nCheck your Downloads folder for:\ninstall-selected-skills.ps1`);

            } catch (error) {
                console.error('[ERROR] Failed to generate download:', error);
                alert('Error generating download: ' + error.message + '\n\nCheck browser console (F12) for details.');
            }
        }

        // Event listeners
        document.getElementById('searchInput').addEventListener('input', renderSkills);

        document.querySelectorAll('.filter-btn').forEach(btn => {
            btn.addEventListener('click', function() {
                document.querySelectorAll('.filter-btn').forEach(b => b.classList.remove('active'));
                this.classList.add('active');
                currentFilter = this.dataset.filter;
                renderSkills();
            });
        });

        // Initialize
        initializeSkills();
    </script>
</body>
</html>
